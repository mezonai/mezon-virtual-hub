{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "seeds/*.json",
        "outDir": "dist"
      }
    ]
  },
  "generateOptions": {
    "flat": false,
    "path": "src/modules"
  }
}
